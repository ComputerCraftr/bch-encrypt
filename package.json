{
  "name": "@anycli/example-multi-js",
  "description": "example multi-command CLI built with javascript",
  "version": "0.9.2",
  "author": "Jeff Dickey @jdxcode",
  "anycli": {
    "commands": "./src/commands",
    "bin": "example-multi-js",
    "plugins": [
      "@anycli/plugin-help",
      "@anycli/plugin-not-found",
      "@anycli/plugin-version"
    ]
  },
  "bin": {
    "example-multi-js": "./bin/run"
  },
  "bugs": "https://github.com/anycli/example-multi-js/issues",
  "dependencies": {
    "@anycli/command": "^1.2.4",
    "@anycli/config": "^1.1.2",
    "@anycli/dev-cli": "^0.1.5",
    "@anycli/engine": "^0.3.6",
    "@anycli/plugin-help": "^0.6.0",
    "@anycli/plugin-not-found": "^0.1.15",
    "@anycli/plugin-version": "^0.1.34",
    "cli-ux": "^3.3.13"
  },
  "devDependencies": {
    "@anycli/test": "^0.10.9",
    "chai": "^4.1.2",
    "eslint": "^4.17.0",
    "eslint-config-anycli": "^1.3.2",
    "mocha": "^5.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".anycli.manifest.json",
    "/bin",
    "/src"
  ],
  "homepage": "https://github.com/anycli/example-multi-js",
  "keywords": [
    "anycli"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "repository": "anycli/example-multi-js",
  "scripts": {
    "lint": "eslint .",
    "postpublish": "rm .anycli.manifest.json",
    "posttest": "yarn run lint",
    "prepublishOnly": "anycli-dev manifest",
    "test": "mocha --forbid-only \"test/**/*.test.js\""
  }
}
