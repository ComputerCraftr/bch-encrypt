{
  "name": "@anycli/example-multi-js",
  "description": "example multi-command CLI built with javascript",
  "version": "0.9.19",
  "author": "Jeff Dickey @jdxcode",
  "anycli": {
    "commands": "./src/commands",
    "bin": "example-multi-js",
    "plugins": [
      "@anycli/plugin-help"
    ]
  },
  "bin": {
    "example-multi-js": "./bin/run"
  },
  "bugs": "https://github.com/anycli/example-multi-js/issues",
  "dependencies": {
    "@anycli/command": "^1.2.19",
    "@anycli/config": "^1.3.45",
    "@anycli/errors": "^0.2.2",
    "@anycli/plugin-help": "^0.7.2",
    "globby": "^7.1.1"
  },
  "devDependencies": {
    "@anycli/dev-cli": "^0.3.9",
    "@anycli/test": "^0.10.15",
    "chai": "^4.1.2",
    "eslint": "^4.17.0",
    "eslint-config-anycli": "^1.3.2",
    "mocha": "^5.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".anycli.manifest.json",
    "/bin",
    "/src"
  ],
  "homepage": "https://github.com/anycli/example-multi-js",
  "keywords": [
    "anycli"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "repository": "anycli/example-multi-js",
  "scripts": {
    "clean": "rm -f .anycli.manifest.json",
    "lint": "eslint .",
    "postpublish": "yarn run clean",
    "posttest": "yarn run lint",
    "prepublishOnly": "anycli-dev manifest && anycli-dev readme",
    "preversion": "yarn run clean",
    "test": "mocha --forbid-only \"test/**/*.test.js\"",
    "version": "anycli-dev readme && git add README.md"
  }
}
